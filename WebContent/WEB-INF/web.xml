    <servlet-class>pack.mypage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mypage</servlet-name>
    <url-pattern>/mypage</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>sam</display-name>
